
const @{}
  MeshBuilder,
= BABYLON

export async function init_demo({scene}) ::
  // Add and manipulate meshes in the scene
  MeshBuilder.CreateSphere @
    'sphere'
    {diameter:2, diameterY:4}
    scene

  MeshBuilder.CreateSphere @
    'sphereTwo'
    {diameter:2, diameterX:7}
    scene

  return scene